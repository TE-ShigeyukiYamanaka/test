#. type: Plain text
#: docs/T4_Device_SDK/ja/DeviceSDK_Manual_ja.adoc:275
msgid "EVP Hubにログインしトークンを取得する"
msgstr "Login to EVP Hub and obtain token."

#. type: delimited block .
#: docs/T4_Device_SDK/ja/DeviceSDK_Manual_ja.adoc:283
#, no-wrap
msgid ""
"export AUTHTOKEN=$( \\\n"
"curl -s --location --request POST \"$EVPHUB_URL/api/auth/login\" \\\n"
"--header 'Content-Type: \"application/json\"' \\\n"
"--header 'Accept: \"application/json\"' \\\n"
"--data-raw '{ \"username\":\"'\"$EVPHUB_USER\"'\", \"password\":\"'\"$EVPHUB_PASS\"'\" }' \\\n"
"  | tee /dev/stderr | jq -r '.token' )\n"
msgstr ""
"export AUTHTOKEN=$( \\\n"
"curl -s --location --request POST \"$EVPHUB_URL/api/auth/login\" \\\n"
"--header 'Content-Type: \"application/json\"' \\\n"
"--header 'Accept: \"application/json\"' \\\n"
"--data-raw '{ \"username\":\"'\"$EVPHUB_USER\"'\", \"password\":\"'\"$EVPHUB_PASS\"'\" }' \\\n"
"  | tee /dev/stderr | jq -r '.token' )\n"

#. type: Plain text
#: docs/T4_Device_SDK/ja/DeviceSDK_Manual_ja.adoc:286
msgid "EVP HubにModuleを登録し、Module IDを取得する"
msgstr "Register Module to EVP Hub and obtain Module ID."

#. type: delimited block .
#: docs/T4_Device_SDK/ja/DeviceSDK_Manual_ja.adoc:297
#, no-wrap
msgid ""
"export MODULE_ID=$( \\\n"
"curl --location --request POST \"$EVPHUB_URL/api/evp/v1/module\" \\\n"
"--header 'Content-Type: application/json' \\\n"
"--header \"X-Authorization: Bearer: $AUTHTOKEN\" \\\n"
"--data-raw '{\"name\": \"'\"$MODULE_NAME\"\\\n"
"'\",\"type\": \"Linux modules\",\"resourceUrl\":\"'\\\n"
"\"$DOCKER_REGISTRY\"'/'\"$MODULE_IMAGE_NAME\"\\\n"
"'\",\"entryPoint\": \"main\",\"hash\": \"'\"$MODULE_HASH\"'\"}' \\\n"
"  | tee /dev/stderr | jq -r '.id.id' )\n"
msgstr ""
"export MODULE_ID=$( \\\n"
"curl --location --request POST \"$EVPHUB_URL/api/evp/v1/module\" \\\n"
"--header 'Content-Type: application/json' \\\n"
"--header \"X-Authorization: Bearer: $AUTHTOKEN\" \\\n"
"--data-raw '{\"name\": \"'\"$MODULE_NAME\"\\\n"
"'\",\"type\": \"Linux modules\",\"resourceUrl\":\"'\\\n"
"\"$DOCKER_REGISTRY\"'/'\"$MODULE_IMAGE_NAME\"\\\n"
"'\",\"entryPoint\": \"main\",\"hash\": \"'\"$MODULE_HASH\"'\"}' \\\n"
"  | tee /dev/stderr | jq -r '.id.id' )\n"

